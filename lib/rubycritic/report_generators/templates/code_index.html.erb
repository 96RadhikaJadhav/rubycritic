<table id="js-code-table" class="analysis-index-table sortable-table">
<thead>
  <tr>
    <th class="first-cell">Name</th>
    <th>Smells</th>
  </tr>
</thead>
<tbody>
  <% @analysed_files.each do |analysed_file| %>
    <tr>
      <td class="first-cell">
        <a href="<%= file_path(analysed_file.pathname) %>"><%= analysed_file_name(analysed_file.pathname) %></a>
      </td>
      <td><%= analysed_file.smells.length %></td>
    </tr>
  <% end %>
</tbody>
</table>
